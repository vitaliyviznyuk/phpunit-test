{
    "name": "vitaliyviznyuk/phpunit-test",
    "description": "Examples from PHPUnit Manual (https://phpunit.readthedocs.io/en/8.4/index.html).",
    "keywords": ["PHPUnit", "examples", "manual"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Viznyuk",
            "email": "vitaliyviznyuk@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": { "vitaliyviznyuk\\PHPUnitTest\\": "src/" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "8.4"
    }
}
